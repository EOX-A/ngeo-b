<?xml version="1.0" encoding="UTF-8"?>
<bsi:ingestBrowseResponse xsi:schemaLocation="http://ngeo.eo.esa.int/schema/browse/ingestion ../ngEOBrowseIngestionService.xsd"
xmlns:bsi="http://ngeo.eo.esa.int/schema/browse/ingestion" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <bsi:status>{{ result.status }}</bsi:status>
    <bsi:ingestionSummary>
        <bsi:toBeReplaced>{{ result.to_be_replaced }}</bsi:toBeReplaced>
        <bsi:actuallyInserted>{{ result.actually_inserted }}</bsi:actuallyInserted>
        <bsi:actuallyReplaced>{{ result.actually_replaced }}</bsi:actuallyReplaced>
    </bsi:ingestionSummary>
    <bsi:ingestionResult>{% for identifier, status, code, message in result %}
        <bsi:briefRecord>
            <bsi:identifier>{{ identifier }}</bsi:identifier>
            <bsi:status>{{ status }}</bsi:status>{% if code or message %}
            <bsi:error>
                <bsi:exceptionCode>{{ code }}</bsi:exceptionCode>
                <bsi:exceptionMessage>{{ message }}</bsi:exceptionMessage>
            </bsi:error>{% endif %}
        </bsi:briefRecord>{% endfor %}
    </bsi:ingestionResult>
</bsi:ingestBrowseResponse>
